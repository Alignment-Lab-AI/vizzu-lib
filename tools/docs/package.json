  "license": "Apache-2.0",
  "dependencies": {
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2"
  }
}
